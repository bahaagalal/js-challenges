<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<title>JS Challenge <%= id %></title>
	<link type="text/css" rel="stylesheet" href="/prism.css">
</head>
<body>
	<h1>JS Challenge #<%= id %></h1>
	<code class="language-javascript" style="width: 100%; display: block;">
		<% if(id == 1){ %>
		jQuery(document).ready(function(){
			for(var i = 0; i < 10; i++)
			{
				jQuery.get('http://jsonplaceholder.typicode.com/posts/1', function(data){
					jQuery('#results').append(i + "<br>");
				});
			}
		});
		<% } %>
	</code>
	<a href="#" id="show">Show Results</a>
	<div id="results" style="display:none; margin-top: 20px;">
	</div>
	<script src="//code.jquery.com/jquery-1.11.3.min.js"></script>
	<script src="/prism.js"></script>
	<script>
		jQuery(document).ready(function(){
			jQuery('#show').click(function(){
				jQuery('#results').show();
			});
		});
	</script>
	<script src="/challenges/1.js"></script>
</body>
</html>
